{
	"private": false,
	"scripts": {
		"build": "turbo run build",
		"build-force": "turbo run build --force",
		"dev": "turbo run dev",
		"biome": "pnpm exec biome check --write",
		"biome-hook": "pnpm exec biome check --write --no-errors-on-unmatched --files-ignore-unknown=true --colors=off",
		"clean": "turbo run clean && rm -rf node_modules",
		"changeset": "changeset",
		"version-packages": "changeset version",
		"release": "turbo run build --filter=@gatewai/guides^... && changeset publish",
		"start": "turbo run start-cli",
		"lefthook-install": "lefthook install"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.8",
		"@changesets/cli": "^2.3.0",
		"dotenv": "^17.2.3",
		"lefthook": "^2.0.11",
		"turbo": "^2.5.4"
	},
	"packageManager": "pnpm@10.13.1",
	"engines": {
		"node": ">=22"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"gl",
			"canvas"
		]
	},
	"name": "gatewai"
}
