{
	"private": false,
	"scripts": {
		"build": "turbo run build",
		"be:build": "turbo run be:build --filter=@gatewai/fe",
		"dev": "turbo run dev --concurrency 35",
		"dev-mcp": "turbo run dev --filter=@gatewai/mcp-server",
		"biome": "pnpm exec biome check --write",
		"biome-hook": "pnpm exec biome check --write --no-errors-on-unmatched --files-ignore-unknown=true --colors=off",
		"prune": "turbo run prune",
		"changeset": "changeset",
		"version-packages": "changeset version",
		"release:tags": "changeset version && changeset tag && git tag -a v$(node -p \"require('./apps/gatewai-fe/package.json').version\") -m \"Release v$(node -p \"require('./apps/gatewai-fe/package.json').version\")\" && git push --follow-tags",
		"start-cli": "turbo run start-cli --filter=@gatewai/backend",
		"start-mcp": "turbo run start-cli --filter=@gatewai/mcp-server",
		"db:migrate": "turbo run db:migrate --filter=@gatewai/db",
		"db:generate": "turbo run db:generate --filter=@gatewai/db",
		"db:seed": "turbo run db:seed --filter=@gatewai/db",
		"db:reset": "turbo run db:reset --filter=@gatewai/db",
		"db:deploy": "turbo run db:deploy --filter=@gatewai/db",
		"test": "turbo run test",
		"lefthook-install": "lefthook install",
		"mcp-inspector": "npx @modelcontextprotocol/inspector",
		"start:docker": "docker compose --env-file .env.local -f docker-compose-local.yaml up"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.8",
		"@changesets/cli": "^2.3.0",
		"dotenv": "^17.2.3",
		"lefthook": "^2.0.11",
		"turbo": "^2.5.4",
		"vitest": "^4.0.18"
	},
	"packageManager": "pnpm@10.13.1",
	"engines": {
		"node": ">=22"
	},
	"pnpm": {
		"overrides": {
			"react": "^19.2.4",
			"react-dom": "^19.2.4",
			"@types/react": "^19.2.4",
			"@types/react-dom": "^19.2.3"
		},
		"onlyBuiltDependencies": [
			"@prisma/client",
			"@prisma/engines",
			"@swc/core",
			"canvas",
			"esbuild",
			"lefthook",
			"msgpackr-extract",
			"prisma",
			"protobufjs",
			"sharp",
			"gl"
		]
	},
	"trustedDependencies": [
		"canvas",
		"@swc/core",
		"esbuild",
		"protobufjs",
		"sharp",
		"gl"
	],
	"name": "gatewai",
	"dependencies": {
		"@hookform/resolvers": "^3.10.0",
		"@lobehub/icons": "^2.48.0",
		"clsx": "^2.1.1",
		"file-type": "^21.1.1",
		"lucide-react": "^0.563.0",
		"react-hook-form": "^7.71.1",
		"react-icons": "^5.5.0",
		"tailwind-merge": "^3.4.0",
		"zod": "^3.24.1"
	}
}
