{
	"private": false,
	"scripts": {
		"build": "turbo run build",
		"be:build": "turbo run be:build --filter=@gatewai/fe",
		"dev": "turbo run dev",
		"dev-mcp": "turbo run dev --filter=@gatewai/mcp-server",
		"biome": "pnpm exec biome check --write",
		"biome-hook": "pnpm exec biome check --write --no-errors-on-unmatched --files-ignore-unknown=true --colors=off",
		"clean": "turbo run clean && rm -rf node_modules",
		"changeset": "changeset",
		"version-packages": "changeset version",
		"release": "changeset publish",

		"start-cli": "turbo run start-cli --filter=@gatewai/fe",
		"start-mcp": "turbo run start-cli --filter=@gatewai/mcp-server",

		"db:migrate": "turbo run db:migrate --filter=@gatewai/db",
		"db:generate": "turbo run db:generate --filter=@gatewai/db",
		"db:seed": "turbo run db:seed --filter=@gatewai/db",
		"db:reset": "turbo run db:reset --filter=@gatewai/db",
		"db:deploy": "turbo run db:deploy --filter=@gatewai/db",

		"lefthook-install": "lefthook install",
		"mcp-inspector": "npx @modelcontextprotocol/inspector"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.8",
		"@changesets/cli": "^2.3.0",
		"dotenv": "^17.2.3",
		"lefthook": "^2.0.11",
		"turbo": "^2.5.4"
	},
	"packageManager": "pnpm@10.13.1",
	"engines": {
		"node": ">=22"
	},
	"pnpm": {
		"overrides": {
			"react": "^19.2.0",
			"react-dom": "^19.2.0"
		},
		"onlyBuiltDependencies": [
			"@prisma/client",
			"@prisma/engines",
			"@swc/core",
			"canvas",
			"esbuild",
			"lefthook",
			"msgpackr-extract",
			"prisma",
			"protobufjs",
			"sharp",
			"gl"
		]
	},
	"trustedDependencies": [
		"canvas",
		"@swc/core",
		"esbuild",
		"protobufjs",
		"sharp",
		"gl"
	],
	"name": "gatewai"
}
