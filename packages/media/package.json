{
	"name": "@gatewai/media",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"sideEffects": false,
	"license": "MIT",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.mts",
	"exports": {
		".": {
			"types": "./dist/index.d.mts",
			"development": "./src/index.ts",
			"import": "./dist/index.mjs",
			"default": "./dist/index.mjs"
		}
	},
	"files": [
