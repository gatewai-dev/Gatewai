	},
	"devDependencies": {
		"@gatewai/tsconfig": "workspace:*",
		"@types/node": "^24.0.4",
		"@types/sharp": "^0.31.1",
		"tsdown": "^0.19.0",
		"typescript": "5.5.4"
	},
	"dependencies": {
		"@gatewai/core": "workspace:*",
		"@gatewai/types": "workspace:*",
		"@gatewai/pixi-processor": "workspace:*",
		"@pixi/node": "^7.3.0",
		"pixi.js": "^7.4.3",
		"sharp": "^0.34.5",
		"canvas": "^3.2.0"
	}
}
