{
	"name": "@gatewai/nodes",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"sideEffects": false,
	"license": "MIT",
	"description": "Built-in node manifests for Gatewai",
	"module": "./dist/index.mjs",
	"types": "./dist/server.d.mts",
	"exports": {
		".": {
			"types": "./dist/server.d.mts",
			"development": "./src/server/index.ts",
			"import": "./dist/server.mjs",
			"default": "./dist/server.mjs"
		},
		"./server": {
			"types": "./dist/server.d.ts",
			"development": "./src/server/index.ts",
			"import": "./dist/server.mjs",
			"default": "./dist/server.mjs"
		},
		"./browser": {
			"types": "./dist/browser.d.ts",
			"development": "./src/browser/index.ts",
			"import": "./dist/browser.mjs",
			"default": "./dist/browser.mjs"
		},
		"./configs": {
			"types": "./dist/configs.d.ts",
			"development": "./src/index.ts",
			"import": "./dist/configs.mjs",
			"default": "./dist/configs.mjs"
		}
	},
	"files": [
		"dist/**"
	],
	"scripts": {
		"build": "tsdown --format esm",
		"dev": "tsdown --format esm --watch",
		"clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
	},
	"dependencies": {
		"@gatewai/core": "workspace:*",
		"@gatewai/db": "workspace:*",
		"@gatewai/node-sdk": "workspace:*",
		"@gatewai/media": "workspace:*",
		"@gatewai/react-store": "workspace:*",
		"@google/genai": "^1.40.0",
		"music-metadata": "^10.6.5",
		"reflect-metadata": "^0.2.2",
		"tsyringe": "^4.10.0",
		"wav": "^1.0.2",
		"zod": "3.25.67"
	},
	"devDependencies": {
		"@gatewai/tsconfig": "workspace:*",
		"@types/node": "^24.0.4",
		"@types/wav": "^1.0.4",
		"tsdown": "^0.19.0",
		"typescript": "5.5.4"
	},
	"peerDependencies": {
		"react": "^18.0.0 || ^19.0.0",
		"react-dom": "^18.0.0 || ^19.0.0"
	}
}