{
	"name": "@gatewai/pixi-processor",
	"version": "0.0.19",
	"private": true,
	"type": "module",
	"sideEffects": false,
	"license": "MIT",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.mts",
	"exports": {
		".": {
			"types": "./dist/index.d.mts",
			"development": "./src/index.ts",
			"import": "./dist/index.mjs"
		}
	},
	"files": [
		"dist/**"
	],
	"scripts": {
		"build": "tsdown src/index.ts --format esm",
		"dev": "tsdown src/index.ts --format esm --watch",
		"clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
	},
	"devDependencies": {
		"@gatewai/tsconfig": "workspace:*",
		"@gatewai/types": "workspace:*",
		"pixi-filters": "^5.3.0",
		"pixi.js": "^7.4.3",
		"@pixi/node": "^7.3.0",
		"tsdown": "^0.19.0",
		"typescript": "5.5.4"
	},
	"dependencies": {
		"generic-pool": "^3.9.0",
		"p-limit": "^7.2.0",
		"zod": "3.25.67"
	}
}
