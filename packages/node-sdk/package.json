{
	"name": "@gatewai/node-sdk",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"sideEffects": false,
	"license": "MIT",
	"description": "SDK for building Gatewai node plugins",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.mts",
	"exports": {
		".": {
			"types": "./dist/index.d.mts",
			"development": "./src/index.ts",
			"import": "./dist/index.mjs",
			"default": "./dist/index.mjs"
		},
		"./dist/index.mjs": "./dist/index.mjs"
	},
	"files": [
		"dist/**"
	],
	"scripts": {
		"build": "tsdown src/index.ts --format esm",
		"dev": "tsdown src/index.ts --format esm --watch",
		"clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
	},
	"devDependencies": {
		"@gatewai/tsconfig": "workspace:*",
		"@types/node": "^24.0.4",
		"tsdown": "^0.19.0",
		"typescript": "5.5.4"
	},
	"peerDependencies": {
		"@gatewai/core": "workspace:*",
		"@gatewai/db": "workspace:*",
		"@gatewai/pixi-processor": "workspace:*",
		"@gatewai/types": "workspace:*",
		"zod": "3.25.67"
	},
	"dependencies": {
		"reflect-metadata": "^0.2.2",
		"tsyringe": "^4.10.0"
	}
}
