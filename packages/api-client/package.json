{
	"name": "@gatewai/api-client",
	"version": "0.0.0",
	"type": "module",
	"sideEffects": false,
	"license": "MIT",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.mts",
	"exports": {
		".": {
			"types": "./dist/index.d.mts",
			"development": "./src/index.ts",
			"import": "./dist/index.mjs"
		}
	},
	"files": [
		"dist/**"
	],
	"scripts": {
		"build": "tsdown src/index.ts --format esm",
		"dev": "tsdown src/index.ts --format esm --watch",
		"lint": "eslint \"src/**/*.ts*\"",
		"clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
	},
	"devDependencies": {
		"@gatewai/tsconfig": "workspace:*",
		"@gatewai/types": "workspace:*",
		"@types/node": "^24.0.4",
		"tsdown": "^0.17.0-beta.3",
		"typescript": "5.5.4"
	},
	"dependencies": {
		"hono": "^4.8.5",
		"zod": "3.25.67"
	}
}
