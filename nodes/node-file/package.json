{
	"name": "@gatewai/node-file",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.mts",
	"exports": {
		".": {
			"types": "./dist/index.d.mts",
			"import": "./dist/index.mjs",
			"default": "./dist/index.mjs"
		},
		"./server": {
			"types": "./dist/server.d.mts",
			"import": "./dist/server.mjs",
			"default": "./dist/server.mjs"
		},
		"./browser": {
			"types": "./dist/browser.d.mts",
			"import": "./dist/browser.mjs",
			"default": "./dist/browser.mjs"
		}
	},
	"scripts": {
		"build": "tsdown",
		"dev": "tsdown --watch",
		"clean": "rm -rf dist"
	},
	"dependencies": {
		"zod": "^3.24.1"
	},
	"peerDependencies": {
		"@gatewai/core": "workspace:*",
		"@gatewai/db": "workspace:*",
		"@gatewai/node-sdk": "workspace:*",
		"react": "^19.0.0",
		"tsyringe": "^4.8.0"
	},
	"devDependencies": {
		"@gatewai/tsconfig": "workspace:*",
		"tsdown": "^0.19.0",
		"typescript": "^5.8.3"
	}
}
