		"@gatewai/tsconfig": "workspace:*",
		"@types/react": "^19.2.4",
		"@types/react-dom": "^19.2.4",
		"@types/node": "^22.13.10",
		"tsdown": "^0.19.0",
		"typescript": "~5.8.3"
	},
	"peerDependencies": {
		"react": "^19.2.4",
		"@gatewai/react-canvas": "workspace:*",
