	},
	"peerDependencies": {
		"@gatewai/media": "workspace:*",
		"@hono/zod-validator": "*",
		"@xyflow/react": "^12.8.4",
		"hono": "*",
		"lucide-react": "^0.552.0",
		"inversify": "^7.11.0",
		"react": "^19.2.4",
		"react-dropzone": "*",
		"react-icons": "*",
		"sonner": "*"
	},
	"devDependencies": {
		"@gatewai/react-canvas": "workspace:*",
		"@gatewai/tsconfig": "workspace:*",
		"@types/node": "^24.0.4",
		"@types/react": "^19.2.4",
		"tsdown": "^0.19.0",
		"typescript": "^5.8.3"
	}
