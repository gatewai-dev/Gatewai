{
	"name": "@gatewai/backend",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"types": "./dist/src/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/src/index.d.ts",
			"default": "./dist/src/index.js"
		}
	},
	"scripts": {
		"dev": "tsx watch --tsconfig tsconfig.json --conditions=development src/index.ts",
		"build": "tsc -b tsconfig.json && pnpm copy-assets",
		"copy-assets": "mkdir -p dist && cp -r src/assets dist/",
		"start-cli": "node dist/src/index.js",
		"test": "vitest run"
	},
	"dependencies": {
		"@ai-sdk/google": "^3.0.16",
		"@gatewai/api-client": "workspace:*",
		"@gatewai/core": "workspace:*",
		"@gatewai/data-ops": "workspace:*",
		"@gatewai/node-sdk": "workspace:*",
		"@gatewai/db": "workspace:*",
		"@gatewai/graph-engine": "workspace:*",
		"@gatewai/media": "workspace:*",
		"@gatewai/tsconfig": "workspace:*",
		"@google-cloud/secret-manager": "^6.1.1",
		"@google/adk": "^0.2.4",
		"@google/genai": "1.32.0",
		"@hono/node-server": "^1.17.1",
		"@hono/zod-validator": "^0.7.2",
		"@openai/agents": "^0.4.1",
		"@openai/agents-extensions": "^0.4.1",
		"ai": "^6.0.58",
		"better-auth": "^1.4.17",
		"bullmq": "^5.66.4",
		"dotenv": "^17.0.1",
		"file-type": "^21.1.1",
		"generic-pool": "^3.9.0",
		"gl": "^6.0.2",
		"hono": "^4.8.5",
		"ioredis": "^5.8.0",
		"json-schema-to-zod": "^2.7.0",
		"lodash": "^4.17.21",
		"loglevel": "^1.9.2",
		"mime-types": "^3.0.2",
		"music-metadata": "^11.10.3",
		"p-limit": "^7.2.0",
		"pino": "^10.1.0",
		"pino-http": "^11.0.0",
		"query-string": "^9.3.1",
		"quickjs-emscripten": "^0.31.0",
		"reflect-metadata": "^0.2.2",
		"tsyringe": "^4.10.0",
		"zod": "3.25.67",
		"zod-to-json-schema": "^3.25.1"
	},
	"devDependencies": {
